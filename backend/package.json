{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "dev:debug": "nodemon --exec \"tsx --inspect\" src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate:up": "node-pg-migrate up --migrations-dir ./migrations --migration-file-language sql",
    "migrate:down": "node-pg-migrate down --migrations-dir ./migrations --migration-file-language sql",
    "migrate:create": "node-pg-migrate create --migrations-dir ./migrations --migration-file-language sql",
    "migrate:redo": "node-pg-migrate redo --migrations-dir ./migrations --migration-file-language sql",
    "db:setup": "npm run migrate:up",
    "dev:migrate": "npm run migrate:up && npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.2",
    "redis": "^5.5.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.4",
    "@types/pg": "^8.15.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}
